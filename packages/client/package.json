{
  "name": "client",
  "version": "0.0.0",
  "scripts": {
    "dev": "cross-env NODE_ENV=development ts-node --project tsconfig.server.json server/index.ts",
    "build": "yarn build:sw && yarn build:client && yarn build:server",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --outDir dist/server --ssr src/entry-server.tsx",
    "build:sw": "tsc -p tsconfig.sw.json",
    "preview": "ts-node --project tsconfig.server.json server/index.ts",
    "typecheck": "tsc --noEmit",
    "eslint": "eslint .",
    "stylelint": "stylelint \"src/**/*.{css,scss}\"",
    "lint": "eslint . && stylelint \"src/**/*.{css,scss}\"",
    "lint:fix": "eslint . --fix && stylelint \"src/**/*.{css,scss}\" --fix",
    "format": "prettier --write .",
    "test": "jest ./",
    "generate-api": "rtk-query-codegen-openapi src/api/generated/apiConfig.cjs"
  },
  "dependencies": {
    "@ant-design/cssinjs": "^1.24.0",
    "@ant-design/icons": "5.x",
    "@reduxjs/toolkit": "^2.8.2",
    "@types/glob": "^8.1.0",
    "antd": "^5.26.4",
    "cookie-parser": "^1.4.7",
    "dotenv": "^16.0.2",
    "express": "^4.19.2",
    "glob": "^11.0.3",
    "http-proxy-middleware": "^3.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.5",
    "react-redux": "^9.2.0",
    "react-router": "^7.6.3",
    "serialize-javascript": "^6.0.2"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.4.2",
    "@rtk-query/codegen-openapi": "^2.0.0",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^13.3.0",
    "@types/cookie-parser": "^1.4.9",
    "@types/jest": "^28.1.8",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/serialize-javascript": "^5.0.4",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "@vitejs/plugin-react": "^5.0.1",
    "autoprefixer": "^10.4.21",
    "cross-env": "^10.0.0",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^28",
    "jest-canvas-mock": "^2.5.2",
    "jest-environment-jsdom": "^29.0.1",
    "jest-transform-stub": "^2.0.0",
    "lefthook": "^1.3.9",
    "postcss": "^8.5.6",
    "postcss-custom-media": "^11.0.6",
    "postcss-import": "^16.1.1",
    "prettier": "^3.5.3",
    "stylelint": "^16.21.1",
    "stylelint-config-css-modules": "^4.5.1",
    "stylelint-config-standard": "^38.0.0",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.0",
    "vite": "^7.1.3"
  },
  "license": "MIT"
}
